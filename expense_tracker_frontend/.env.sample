# Frontend environment variables
# Copy this file to .env and adjust as needed.

# Base URL of the backend API (e.g., http://localhost:8080 or your preview URL)
REACT_APP_API_URL=http://localhost:8080

# Comma-separated feature flags (e.g., "reports,goals")
REACT_APP_FEATURE_FLAGS=reports,goals

# UI theme: "light" or "dark" (optional; app also supports toggle)
REACT_APP_THEME=light
